                                            <?php else: ?>
                                                <?php if(isset($voucher->currentApprover[0]) &&
                                                        (($voucher->currentApprover[0]->user_name == 'pl-medical-officer' &&
                                                            $employee->user_name == 'pl-medical-officer') ||
